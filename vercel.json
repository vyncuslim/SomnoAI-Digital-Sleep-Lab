
{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/privacy",
      "destination": "/privacy.html"
    },
    {
      "source": "/terms",
      "destination": "/terms.html"
    },
    {
      "source": "/((?!privacy|terms|robots|sitemap|assets|favicon|google|sw|manifest).* )",
      "destination": "/index.html"
    }
  ]
}
