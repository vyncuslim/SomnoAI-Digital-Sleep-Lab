
{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/favicon.png",
      "destination": "https://img.icons8.com/fluency/512/moon-and-stars.png",
      "permanent": true
    },
    {
      "source": "/favicon.ico",
      "destination": "https://img.icons8.com/fluency/48/moon-and-stars.png",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/admin", "destination": "/index.html" },
    { "source": "/admin/:path*", "destination": "/index.html" },
    { "source": "/login", "destination": "/index.html" },
    { "source": "/((?!assets/|fonts/|favicon.ico|favicon.png|manifest.json|sw.js|.*\\.[a-zA-Z0-9]+$).*)", "destination": "/index.html" }
  ]
}
